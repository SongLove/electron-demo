{
  "name": "electron-react-ts-demo",
  "version": "1.0.0",
  "description": "Electron React TypeScript Demo with Ant Design",
  "main": "dist/main/main.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "npm run build && concurrently \"npm run start:renderer\" \"npm run start:electron\"",
    "start:renderer": "webpack serve --config webpack.config.js",
    "start:electron": "wait-on tcp:3000 && electron .",
    "clean": "rimraf dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.19.86",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "antd": "^5.3.1",
    "buffer": "^6.0.3",
    "electron-is-dev": "^2.0.0",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}